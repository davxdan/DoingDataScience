LoadSleepData <- function(filename="CloneOfAwesomePublicDatasets/Datasets/sleep_data_01.csv")
{
library(readr)
sleep_data_01 <- read_csv(filename)
sdRSES<-sapply(na.omit(sleep_data_01[12]),sd) #Get SD of RSES
meanRSES<-sapply(na.omit(sleep_data_01[12]),mean) #Get Median of RSES
mdnAge<-sapply(na.omit(sleep_data_01[2]),median) #Get ages and ignore na
rangeDuration<-sapply(na.omit(sleep_data_01[15]),range)
cmbRangeDuration<c(rangeDuration[1],rangeDuration[2])
Results<-c("Sd of RSES"= sdRSES,"Mean of RSES"=meanRSES,"Durange"=paste(cmbRangeDuration,sep = "-"))
report<-data.frame(Results)
cnames<-c("SE_SD", "SelfEsteem", "MedianAge", "DurationRange")
report
paste(c(1, 2), sep = "-")
}
LoadSleepData()
cmbRangeDuration<c(rangeDuration[1],rangeDuration[2])
LoadSleepData()
